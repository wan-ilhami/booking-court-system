<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion" id="accordionSidebar">

            <!-- Sidebar - Brand -->
            <a class="sidebar-brand d-flex align-items-center justify-content-center" href="/userdashboard">
                
                <div class="sidebar-brand-text mx-3">Booking Court System</div>
            </a>

            <!-- Divider -->
            <hr class="sidebar-divider my-0">

            <!-- Nav Item - Dashboard -->
            <li class="nav-item active">
                <a class="nav-link" href="/userdashboard">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Dashboard</span></a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider">

            <li class="nav-item">
                <a class="nav-link" href="/profile">
                    <i class="fa fa-user"></i>
                    <span>User Profile</span></a>
            </li>
            <hr class="sidebar-divider">
            <!-- Nav Item - Charts -->
            <li class="nav-item">
                <a class="nav-link" href="/bookingcourt">
                    <i class="fas fa-fw fa-chart-area"></i>
                    <span>Booking Court</span></a>
            </li>
            <hr class="sidebar-divider">
            <!-- Nav Item - Tables -->
            <li class="nav-item">
                <a class="nav-link" href="/historybooking">
                    <i class="fas fa-fw fa-table"></i>
                    <span>History Booking</span></a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider d-none d-md-block">

            <!-- Sidebar Toggler (Sidebar) -->
            <div class="text-center d-none d-md-inline">
                <button class="rounded-circle border-0" id="sidebarToggle"></button>
            </div>

           

        </ul>
        <!-- End of Sidebar -->

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">
                    <!-- Topbar Navbar -->
                    <span>User Dashboard</span>
                    <ul class="navbar-nav ml-auto">
                        <!-- Nav Item - User Information -->
                            <button class="w3-button w3-hover-light-grey"><a href="/logout">Log out</a></button>
                    </ul>
                </nav>
                <!-- End of Topbar -->
<div class="row">


    <div class="card shadow mb-4" style="width: 40%; margin:auto; display:block;">

                <div class="card-header py-3" style="text-align:center;">
                    <h3>My Profile</h3>
                </div>
                <div class="card-body">
                   <form action="/updprofile" method="post">
                        <div class="jarakInput ">
                            <label for="name"><a>Name : </a></label>
                            <input type="text" name="name" value="<%= name %> " class="form-control form-control-user" >
                        </div>

                        <hr class="sidebar-divider">

                        <div class="jarakInput">
                            <label for="uname"><a>Email :</a></label>
                            <input type="email" name="email" value="<%= email %> "class="form-control form-control-user" >
                        </div>

                        <hr class="sidebar-divider">

                        
                        <!-- belum setel lagi-->
                        <input type="submit"   class="btn btn-primary btn-user btn-block" onclick="return confirm('Are you confirm?')">
                            
                        </input>
                    </form>
                    
                </div>
    </div>
</div>






   

    
    <!-- Bootstrap core JavaScript-->
    <script src="/vendor/jquery/jquery.min.js"></script>
